ANS 23:
#include <stdio.h>

int main(){

	int t;
	scanf("%d", &t);

	while(t--){
		int g,p;
		scanf("%d %d", &g, &p);

	int n;
	scanf("%d", &n);

	int x=0, y=0;

	for(int i=0;i<n;i++){
		int a, b;
		scanf("%d %d", &a, &b);
		if(a==1)x++;
		if(b==1)y++;
	}

	int cost1 = x*g + y*p;
	int cost2 = x*p + y*g;

	if(cost1 < cost2)
		printf("%d\n", cost1);
	else
	printf("%d\n", cost2);
	}

	
	return 0;

}
